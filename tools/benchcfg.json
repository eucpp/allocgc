{
  "targets": [
    {
      "name": "boehm",
      "alias": "gcbench top-down",
      "runnable": "benchmark/boehm/boehm",
      "suites": ["gc_ptr", "gc_ptr incremental"],
      "params": ["top-down"],
      "reports": ["tex-table"]
    },
    {
      "name": "boehm",
      "alias": "gcbench bottom-up",
      "runnable": "benchmark/boehm/boehm",
      "suites": ["gc_ptr", "gc_ptr incremental"],
      "params": ["bottom-up"],
      "reports": ["tex-table"]
    },
    {
      "name": "parallel_merge_sort",
      "alias": "parallel merge sort",
      "runnable": "benchmark/parallel_merge_sort/parallel_merge_sort",
      "suites": ["gc_ptr", "gc_ptr incremental"],
      "reports": ["tex-table"]
    },
    {
      "name": "cord",
      "alias": "cord-build",
      "runnable": "benchmark/cord/cord",
      "suites": ["gc_ptr", "gc_ptr incremental"],
      "params": ["build", {"len": [6]}],
      "reports": ["tex-table"]
    },
    {
      "name": "cord",
      "alias": "cord-substr",
      "runnable": "benchmark/cord/cord",
      "suites": ["shared_ptr", "BDW GC", "BDW GC incremental", "gc_ptr", "gc_ptr incremental"],
      "params": ["substr", {"len": [6]}],
      "reports": ["tex-table"]
    },
    {
      "name": "cord",
      "alias": "cord-flatten",
      "runnable": "benchmark/cord/cord",
      "suites": ["gc_ptr", "gc_ptr incremental"],
      "params": ["flatten", {"len": [5]}],
      "reports": ["tex-table"]
    }
  ],
  "suites": [
    {"name": "gc_ptr", "builder": "gc_ptr"},
    {"name": "gc_ptr incremental", "builder": "gc_ptr", "args": ["--incremental"]}
  ],
  "builders": [
    {"name": "gc_ptr", "type": "cmake", "options": ["PRECISE_GC"]}
  ],
  "reporters": [
    {"name": "tex-table"}
  ],
  "nruns" : 20
}